<%= form_for @schedule,:url => {:action => :create} do |f| %>
  <p>
  <%= f.label :start_date %>
  <%= f.text_field :start_date %>
  </p>
 
  <p>
  <%= f.label :end_date %>
  <%= f.text_field :end_date %>
  </p>
 
  <p>
  <%= f.label :schedule %>
  <%= f.text_field :schedule %>
  </p>
  
  <%= f.submit %>
 
<% end %>